  "dependencies": {
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltestrap/sveltestrap": "^7.1.0",
    "bootstrap": "^5.3.6",
    "chart.js": "^4.4.9",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.2.0",
    "echarts": "^5.6.0",
    "express": "^4.21.2",
    "highcharts": "^12.2.0",
    "nedb": "^1.8.0",
    "node-fetch": "^3.3.2",
    "request": "^2.88.2",
    "svelte": "^5.27.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@rollup/rollup-win32-x64-msvc": "4.44.0",
    "@types/node": "^22.14.1",
    "rollup": "^4.17.0",
    "start-server-and-test": "^2.0.11",
    "vite": "^6.3.0"
  }
